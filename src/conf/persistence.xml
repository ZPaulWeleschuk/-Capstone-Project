<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="WorkOrderPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <non-jta-data-source>java:/comp/env/jdbc/workorderdb</non-jta-data-source>
    <class>models.Cause</class>
    <class>models.Damage</class>
    <class>models.Plant</class>
    <class>models.Notification</class>
    <class>models.Operations</class>
    <class>models.TechnicalObject</class>
    <class>models.WorkOrder</class>
    <class>models.WorkCenter</class>
    <class>models.NotifType</class>
    <class>models.OrderType</class>
    <class>models.Status</class>
    <class>models.Roles</class>
    <class>models.Users</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
  </persistence-unit>
</persistence>
